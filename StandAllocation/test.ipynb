          "   \"outputs\": [],\n",
          "   \"source\": [\n",
          "    \"#import folium and set coordinates\\n\",\n",
          "    \"import folium\\n\",\n",
          "    \"gjson = r'https://cocl.us/sanfran_geojson'\\n\",\n",
          "    \"sf_map = folium.Map(location = [37.77, -122.42], zoom_start = 12)\"\n",
